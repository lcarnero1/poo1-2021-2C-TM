public class Ejercicio4 {

	public static void main(String[] args) {

		esPar();

	}

	private static void esPar() {
		int numero = 345;
		boolean esPar;
		esPar = numero % 2 == 0;
		System.out.println(esPar);
	}

	// syso ctl + space
