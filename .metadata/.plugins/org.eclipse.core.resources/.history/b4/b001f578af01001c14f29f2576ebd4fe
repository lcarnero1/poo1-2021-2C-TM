package fizzBuzz;

public class FizzBuzz {

	public static void main(String[] args) {

		for (int i = 1; i <= 100; i++) {

			String valor = Integer.toString(i);
			if(i%3 == 0) valor = "Fizz";
			if(i%5 == 0) valor = "Buzz";
			if(i%15 == 0) valor = "FizzBuzz";
			System.out.println(valor);

		}

	}

}
