package ejercicios.clase2;

import java.util.Arrays;
import java.util.Scanner;

public class Eje2Control {

	public static void main(String[] args) {
		int a = 14;
		int b = 13;
		int c = 12;

		int v [] = {a,b,c};
		
		Arrays.sort(v);
		
		for(int i=0; i < v.length; i++)
			System.out.println(v[i]);
		}

}
